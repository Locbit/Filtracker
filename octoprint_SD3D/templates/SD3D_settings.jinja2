<form class="form-horizontal">
   <div class="control-group">
        {% include "snippets/settings/SD3D/sd3d.jinja2" %}

        <label class="control-label">{{ _('Material') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.material">
        </div>
        <label class="control-label">{{ _('Material Diameter') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.diameter">
        </div>
        <label class="control-label">{{ _('Color') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.color">
        </div>
        <label class="control-label">{{ _('Initial Length') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.initial_length">
        </div>
        <label class="control-label">{{ _('Length') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.length">
        </div>
        <label class="control-label">{{ _('MUID') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.muid">
        </div>
       <label class="control-label">{{ _('Job Progress') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.jobProgress">
        </div>
    </div>
</form>
