<form class="form-horizontal">
    <div class="control-group">
        <label class="control-label">{{ _('Device ID') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.did">
        </div>
        <label class="control-label">{{ _('Octoprint Api Key') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.apiKey">
        </div>
        <label class="control-label">{{ _('SD3D Api Key') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.sd3dAPIKey">
        </div>
        <label class="control-label">{{ _('SD3D Access ID') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.sd3dAccessID">
        </div>
        <label class="control-label">{{ _('Desired Layer Height in mm') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.layerHeight">
        </div>
        <label class="control-label">{{ _('Fill density percentage (1-100)') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.fillDensity">
        </div>
        <div class="control-group">
            <div class="controls">
                <label class="checkbox">
                    <input type="checkbox" data-bind="checked: settings.plugins.SD3D.sharingMode"> {{ _('Enable information sharing with SD3D (Sharing Mode)') }}
                </label>
            </div>
        </div>
        <div class="control-group">
            <div class="controls">
                <label class="checkbox">
                    <input type="checkbox" data-bind="checked: settings.plugins.SD3D.cloudMode"> {{ _('Enable downloading of profiles from SD3D (Cloud Mode)') }}
                </label>
            </div>
        </div>
        <div class="control-group">
            <div class="controls">
                <label class="checkbox">
                    <input type="checkbox" data-bind="checked: settings.plugins.SD3D.autoPrintMode"> {{ _('Enable automatic slicing and printing of uploaded STL files') }}
                </label>
            </div>
        </div>
        <label class="control-label">{{ _('Material') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.material">
        </div>
        <label class="control-label">{{ _('Material Diameter') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.diameter">
        </div>
        <label class="control-label">{{ _('Color') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.color">
        </div>
        <label class="control-label">{{ _('Initial Length') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.initial_length">
        </div>
        <label class="control-label">{{ _('Length') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.length">
        </div>
        <label class="control-label">{{ _('MUID') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.muid">
        </div>
       <label class="control-label">{{ _('Job Progress') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.SD3D.jobProgress">
        </div>
    </div>
</form>
